<h1><%=t :Website_Admins %></h1>

<table>
	<tr>
		<th><%=t :Name %></th>
		<th><%=t :Admin_Rights %></th>
	</tr>
	
	<% @website_admins.each do |website_admin| %>
		<tr>
			<td><%= website_admin.person.firstname %> <%=website_admin.person.lastname%></td>
			<td><%= website_admin.adminrights %></td>
			<td><%= link_to t('Profile'), person_path(website_admin.person) %></td>
			<td><%= link_to t('Delete'), website_admin_path(website_admin),
							method: :delete,
							data: {confirm: t('Are_you_sure')} %></td>
		</tr>
	<%end%>
</table>
<br>
<%= link_to t('home'), controller: 'home_page' %>